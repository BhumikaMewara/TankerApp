<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kookyapps.gpstankertracking">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppThemeNoBar"
        android:usesCleartextTraffic="true">
        <activity android:name=".SplashActivity"></activity>
        <activity android:name=".TripDetails" />
        <activity android:name=".TripComplete" />
        <activity android:name=".BookingDetails" />
        <activity android:name=".EnterOTP" />
        <activity android:name=".TankerStartingPic" />
        <activity android:name=".RequestDetails" />
        <activity android:name=".Notifications" />
        <activity android:name=".FirstActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />
    </application>

